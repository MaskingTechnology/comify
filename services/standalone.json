{
    "url": "http://127.0.0.1:3000",
    "setUp": [
        "./integrations/database/setUpMongoDb",
        "./integrations/filestorage/setUpMinioFS",
        "./integrations/authentication/setUpOpenId",
        "./integrations/notification/setUpMemory"
    ],
    "tearDown": [
        "./integrations/notification/tearDown",
        "./integrations/authentication/tearDown",
        "./integrations/filestorage/tearDown",
        "./integrations/database/tearDown"
    ],
    "healthChecks": [
        "./integrations/jitar/notificationHealthCheck",
        "./integrations/jitar/fileStorageHealthCheck",
        "./integrations/jitar/databaseHealthCheck"
    ],
    "standalone":
    {
        "assets": ["index.html", "main.js", "App.js", "vite.svg", "assets/**/*"],
        "middlewares": [
            "./integrations/jitar/authenticationMiddleware"
        ],
        "overrides": {
            "./integrations/database/implementation": "./integrations/database/implementations/MongoDb",
            "./integrations/filestorage/implementation": "./integrations/filestorage/implementations/MinioFS",
            "./integrations/authentication/implementation": "./integrations/authentication/implementations/OpenId",
            "./integrations/notifications/implementation": "./integrations/notifications/implementations/MemoryNotifications"
        }
    }
}
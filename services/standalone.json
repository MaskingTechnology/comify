{
    "url": "http://127.0.0.1:3000",
    "setUp": [
        "./integrations/scripts/setUpBff",
        "./integrations/scripts/setUpWorker"
    ],
    "tearDown": [
        "./integrations/scripts/tearDownWorker",
        "./integrations/scripts/tearDownBff"
    ],
    "healthChecks": [
        "./integrations/healthchecks/fileStoreHealthCheck",
        "./integrations/healthchecks/databaseHealthCheck"
    ],
    "middleware": [
        "./integrations/middlewares/originMiddleware",
        "./integrations/middlewares/authenticationMiddleware",
        "./integrations/middlewares/tenantMiddleware"
    ],
    "standalone":
    {
        "segments": ["bff", "notification", "reads", "writes"],

        "serveIndexOnNotFound": true,
        "assets": ["index.html", "assets/**/*", "webui/**/*", "registerSW.js", "sw.js", "workbox-*.js", "manifest.webmanifest" ]
    }
}
{
  "name": "comify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copy-assets": "cpx src/assets/**/* dist/assets",
    "build": "vite build && tsc -p tsconfig.build.json && npm run copy-assets",
    "clean": "rm -rf dist",
    "docker-up": "docker compose --env-file development.env up",
    "docker-down": "docker compose --env-file development.env down",
    "standalone": "node --experimental-network-imports --env-file=development.env dist/jitar.js --config=services/standalone.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "changelog": "auto-changelog --template changelog.hbs -p -u --commit-limit false --hide-empty-releases true",
    "changelog-debug": "auto-changelog --template changelog.hbs -p --template json --output changelog-data.json",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "dependencies": {
    "dayjs": "^1.11.10",
    "jitar": "^0.7.3",
    "minio": "^7.1.3",
    "mongodb": "^6.4.0",
    "openid-client": "^5.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@jitar/plugin-vite": "^0.7.1",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "auto-changelog": "^2.4.0",
    "cpx2": "^7.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "typescript": "5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
